AddType text/cache-manifest .appcache

RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_URI} !.*proxy.php.*
RewriteRule ^(.*)$ /index.html [L]

#ExpiresActive On
#ExpiresByType text/cache-manifest "access"



